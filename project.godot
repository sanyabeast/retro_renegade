; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Retro Renegade"
config/version="0.1.0"
run/main_scene="res://scenes/levels/_practice_range.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
run/max_fps=120
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

tools="*res://scripts/core/tools.gd"
app="*res://scripts/core/app_manager.gd"
world="*res://scripts/core/world_manager.gd"
camera_manager="*res://scripts/core/camera_manager.gd"
players="*res://scripts/core/player_manager.gd"
npcs="*res://scripts/core/npc_manager.gd"
hud="*res://scripts/core/hud_manager.gd"
game="*res://scripts/core/game_manager.gd"
dev="*res://scripts/_dev/_dev_manager.gd"

[display]

window/size/viewport_width=1600
window/size/viewport_height=900

[editor_plugins]

enabled=PackedStringArray()

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://assets/": "pink",
"res://resources/": "teal",
"res://scenes/": "purple",
"res://scenes/levels/": "gray",
"res://scenes/main/": "gray",
"res://scripts/": "red",
"res://scripts/core/": "gray",
"res://scripts/main/": "gray",
"res://scripts/resources/": "gray"
}

[global]

game_config_path="res://resources/config.tres"

[importer_defaults]

